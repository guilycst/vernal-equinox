<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
	default-autowire="byType">
	<bean class="br.veq.autowiring.beans.Datasource">
		<property name="connectionString"
			value="jdbc:mysql://localhost/db" />
	</bean>
	<bean class="br.veq.autowiring.beans.ProductDAO" />
	<bean class="br.veq.autowiring.beans.SaleDAO" />
	<bean class="br.veq.autowiring.beans.UserDAO" />
	<bean class="br.veq.autowiring.beans.Processor" />
</beans>
