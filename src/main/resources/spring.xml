<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="datasource" class="br.veq.beans.FileDatasource">
		<property name="file" value="/arquivos/people.csv" />
	</bean>
	<bean id="printer" class="br.veq.beans.PrinterImpl" />
	<bean id="processor" class="br.veq.beans.Processor">
		<!-- constructor injection -->
		<constructor-arg ref="datasource" index="0"/>
		<constructor-arg ref="printer" index="1"/>
		<!-- setter injection -->
		<property name="printer" ref="printer" />
		<!-- <property name="datasource" ref="datasource" /> -->
	</bean>
</beans>
